import math
p = int(input("Enter a prime number"))
q = int(input("Enter a prime number"))
m = int(input("Enter the number to be encrypted"))
n = p*q
phi = (p-1)*(q-1)

e = 2
while e<phi:
    if math.gcd(e,phi) == 1:
        break
    else:
        e = e+1

d = pow(e,-1,phi)

print("e:", e)
print("d:", d)

en = pow(m,e,n)
de = pow(en,d,n)

print("encryption:", en)
print("decryption:",Â de)
